fact n : Float => Float = (if n == 0.0 then 1.0 else n * fact ((n) - 1.0))
ffact a: Float -> b: Float => Float = (fact a) * b
call fn : Fn {Float => Float} => Float = fn 3.0
bla => IO = (println "Hello World") >> (print "The factorial of 6.0 is ") >> (fact 6.0) ~> (println ())
